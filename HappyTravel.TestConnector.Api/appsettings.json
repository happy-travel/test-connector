{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authority": {
    "Options": "test-connector/authority"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "test-connector",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Telemetry": {
    "IsEnabled": false
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  }
}
